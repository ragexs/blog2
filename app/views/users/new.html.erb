<h1>Users#new</h1>
<p>
  <%= form_for(@user) do |f| %>
     <%= f.label :name %>
     <%= f.text_field :name %>
</p>
<p>
  <%= f.label :email %>
  <%= f.text_field :email %>
</p>
      <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
      </p>
      <%= f.label :password_confirmation %>
      <%= f.password_field :password_confirmation %>
<p>
  <%= f.submit "Create my account" %>
</p>
<% end %>
